{
    "name": "drupal/drupal",
    "description": "Drupal is an open source content management platform powering millions of websites and applications.",
    "type": "project",
    "license": "GPL-2.0+",
    "require": {
        "drupal/core": "~8.2",
        "composer/installers": "^1.0.21",
        "drupal/address": "1.x-dev",
        "drupal/admin_toolbar": "^1.17",
        "drupal/ajax_comments": "1.x-dev",
        "drupal/config_update": "^1.1",
        "drupal/console": "~1.0",
        "drupal/devel": "1.x-dev",
        "drupal/entity": "1.x-dev",
        "drupal/features": "3.x-dev",
        "drupal/geocoder": "2.x-dev",
        "drupal/geofield": "1.0-alpha2",
        "drupal/geophp": "1.x-dev",
        "drupal/group": "1.x-dev",
        "drupal/profile": "1.x-dev",
        "drupal/token": "1.x-dev",
        "symfony/dom-crawler": "2.8.11",
        "wikimedia/composer-merge-plugin": "~1.3",
        "drupal/yamlform": "1.0-beta25",
        "drupal/default_content": "1.0-alpha3",
        "drupal/chosen": "2.1",
        "drupal/select_or_other": "1.x-dev",
        "drupal/leaflet": "1.x-dev",
        "drupal/field_group": "1.0-rc6",
        "drupal/field_states_ui": "1.x-dev",
        "doctrine/common": "2.5.3",
        "drupal/pathauto": "^1.0@RC",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/smtp": "^1.0@beta",
        "drupal/email_registration": "^1.0@RC"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8"
    },
    "extra": {
        "_readme": [
            "By default Drupal loads the autoloader from ./vendor/autoload.php.",
            "To change the autoloader you can edit ./autoload.php."
        ],
        "merge-plugin": {
            "include": [
                "core/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "merge-extra": false
        },
        "patches": {
            "drupal/default_content": {
                "Allow manual imports": "https://www.drupal.org/files/issues/allow_manual_imports-2640734-11.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Drupal\\Core\\Composer\\Composer::preAutoloadDump",
        "post-autoload-dump": "Drupal\\Core\\Composer\\Composer::ensureHtaccess",
        "post-package-install": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "post-package-update": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "leaflet": {
            "type": "package",
            "package": {
                "name": "leaflet/leaflet",
                "version": "v0.7.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/Leaflet/Leaflet/archive/v0.7.7.zip",
                    "type": "zip"
                }
            }
        }
    }
}
